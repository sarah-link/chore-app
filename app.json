{
  "expo": {
    "name": "Chores",
    "slug": "chore-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/chore_icon_round.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/assets/chore_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fafaf9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "public/assets/chore_icon_round.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.slink2647.choreapp"
    },
    "web": {
      "favicon": "public/assets/favicon.png"
    }
  }
}
